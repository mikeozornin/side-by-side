{
  "name": "side-by-side-server",
  "version": "1.0.0",
  "description": "Backend для side-by-side",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun dist/index.js",
    "db:migrate": "bun src/db/migrate.ts",
    "db:migrate:add-is-public": "bun src/db/migrate.ts add-is-public",
    "db:check-compatibility": "bun src/db/check-compatibility.ts",
    "db:seed": "bun src/db/seed.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:notifications": "bun src/test-notifications.ts",
    "test:locale": "bun src/test-locale.ts",
    "check:env": "bun src/check-env.ts"
  },
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.15.5",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.3.1",
    "file-type": "^18.7.0",
    "hono": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "sharp": "^0.34.3",
    "uuid": "^9.0.1",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@types/bun": "^1.2.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.0"
  }
}
