{
  "name": "side-by-side-server",
  "version": "1.0.0",
  "description": "Backend для side-by-side",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:migrate:add-is-public": "tsx src/db/migrate.ts add-is-public",
    "db:seed": "tsx src/db/seed.ts",
    "test:notifications": "tsx src/test-notifications.ts",
    "test:locale": "tsx src/test-locale.ts",
    "check:env": "tsx src/check-env.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.1",
    "dotenv": "^16.3.1",
    "file-type": "^18.7.0",
    "hono": "^4.0.0",
    "sharp": "^0.34.3",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/sqlite3": "^3.1.8",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
